{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-native",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-native"
    ],
    "baseUrl": ".",
    "paths": {
      "@stores/*": [
        "stores/*"
      ],
      "@lib/*": [
        "lib/*"
      ]
    }
  },
  "include": [
    "app",
    "stores",
    "lib"
  ],
  "extends": "expo/tsconfig.base"
}
